@model IEnumerable<PrinterTonerEPCwithAuthentication.Models.TonerTotal>

@{
    ViewBag.Title = "DailyDirective";
}

<h2>Toneri - stanje magacina</h2>

<table class="table">
    <tr>
        <th>
            Toner
        </th>
        <th>
            Stanje magacina
        </th>
        <th>
            Minimalna količina
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TotalTonerModel)
            </td>
            <td>
                @item.TonerTotalCount
            </td>
            <td>
                @item.TonerTotalMin
            </td>
        </tr>
    }

</table>
